{
  "name": "fet",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "lint:js": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix --cache",
    "lint:css": "stylelint \"src/**/*.{scss}\" --fix --cache --allow-empty-input",
    "format": "prettier \"src/**/*.{html,json,md,js,jsx,ts,tsx,css,scss}\" --write --cache",
    "dev": "storybook dev -p 8082 --no-open",
    "build:sb": "storybook build --disable-telemetry",
    "start:sb": "npx http-server ./storybook-static -p 8082"
  },
  "dependencies": {
    "ecm": "workspace:*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.5.2",
    "@storybook/addon-backgrounds": "7.5.2",
    "@storybook/addon-docs": "7.5.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-measure": "7.5.2",
    "@storybook/addon-outline": "7.5.2",
    "@storybook/addons": "7.5.2",
    "@storybook/channel-postmessage": "7.5.2",
    "@storybook/channel-websocket": "7.5.2",
    "@storybook/cli": "7.5.2",
    "@storybook/client-api": "7.5.2",
    "@storybook/html": "7.5.2",
    "@storybook/html-vite": "7.5.2",
    "@storybook/preview-web": "7.5.2",
    "@storybook/theming": "7.5.2",
    "storybook": "7.5.2"
  }
}
