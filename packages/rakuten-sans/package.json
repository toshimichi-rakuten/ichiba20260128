{
  "name": "rakuten-sans",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && pnpm run --parallel '/^build:(condensed|rounded|serif|jp2.*)/'",
    "build:condensed": "vite build --config vite.config.rakuten_condensed.ts",
    "build:rounded": "vite build --config vite.config.rakuten_rounded.ts",
    "build:serif": "vite build --config vite.config.rakuten_serif.ts",
    "build:jp2-300": "vite build --config vite.config.rakuten_sans_jp2_300.ts",
    "build:jp2-400": "vite build --config vite.config.rakuten_sans_jp2_400.ts",
    "build:jp2-500": "vite build --config vite.config.rakuten_sans_jp2_500.ts",
    "build:jp2-600": "vite build --config vite.config.rakuten_sans_jp2_600.ts",
    "build:jp2-700": "vite build --config vite.config.rakuten_sans_jp2_700.ts",
    "build:jp2-800": "vite build --config vite.config.rakuten_sans_jp2_800.ts",
    "build:jp2tn-300": "vite build --config vite.config.rakuten_sans_jp2tn_300.ts",
    "build:jp2tn-400": "vite build --config vite.config.rakuten_sans_jp2tn_400.ts",
    "build:jp2tn-500": "vite build --config vite.config.rakuten_sans_jp2tn_500.ts",
    "build:jp2tn-600": "vite build --config vite.config.rakuten_sans_jp2tn_600.ts",
    "build:jp2tn-700": "vite build --config vite.config.rakuten_sans_jp2tn_700.ts",
    "build:jp2tn-800": "vite build --config vite.config.rakuten_sans_jp2tn_800.ts",
    "lint:js": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix --cache",
    "lint:css": "stylelint \"src/**/*.{scss}\" --fix --cache --allow-empty-input",
    "format": "prettier \"src/**/*.{html,json,md,js,jsx,ts,tsx,css,scss}\" --write --cache",
    "dev": "storybook dev -p 8083 --no-open",
    "build:sb": "storybook build --disable-telemetry",
    "start:sb": "npx http-server ./storybook-static -p 8083"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.5.2",
    "@storybook/addon-backgrounds": "7.5.2",
    "@storybook/addon-docs": "7.5.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-measure": "7.5.2",
    "@storybook/addon-outline": "7.5.2",
    "@storybook/addons": "7.5.2",
    "@storybook/channel-postmessage": "7.5.2",
    "@storybook/channel-websocket": "7.5.2",
    "@storybook/cli": "7.5.2",
    "@storybook/client-api": "7.5.2",
    "@storybook/html": "7.5.2",
    "@storybook/html-vite": "7.5.2",
    "@storybook/preview-web": "7.5.2",
    "@storybook/theming": "7.5.2",
    "storybook": "7.5.2"
  }
}
